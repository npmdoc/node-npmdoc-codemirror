{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "buildNpmdoc": "codemirror",
    "description": "api documentation for [codemirror (v5.25.0)](http://codemirror.net) [![npm package](https://img.shields.io/npm/v/npmdoc-codemirror.svg?style=flat-square)](https://www.npmjs.org/package/npmdoc-codemirror) [![travis-ci.org build-status](https://api.travis-ci.org/npmdoc/node-npmdoc-codemirror.svg)](https://travis-ci.org/npmdoc/node-npmdoc-codemirror)",
    "devDependencies": {
        "codemirror": "*",
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "utility2": "kaizhu256/node-utility2#alpha"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmdoc/node-npmdoc-codemirror",
    "keywords": [
        "documentation",
        "codemirror"
    ],
    "license": "MIT",
    "main": "lib.npmdoc_codemirror.js",
    "name": "npmdoc-codemirror",
    "nameAlias": "npmdoc_codemirror",
    "nameOriginal": "npmdoc-codemirror",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmdoc/node-npmdoc-codemirror.git"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "(set -e; npm install \"kaizhu256/node-utility2#alpha\"; utility2 shDeployHeroku)",
        "postinstall": "if [ -f npm_scripts.sh ]; then ./npm_scripts.sh postinstall; fi",
        "start": "(set -e; export PORT=${PORT:-8080}; utility2 start test.js)",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "0.0.2"
}
